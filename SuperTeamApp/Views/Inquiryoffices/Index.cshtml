@model IEnumerable<SuperTeamApp.Models.Inquiryoffice>

@{
    ViewData["Title"] = "Index";
}

<h1>Inquiryoffice</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FoodName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Question)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Answer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FoodName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Answer)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InquiryId">Answer</a> |
                <a asp-action="Details" asp-route-id="@item.InquiryId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InquiryId">Delete</a> |
            </td>
        </tr>
}
    </tbody>
</table>
